#!/bin/bash
cd "$(dirname "$0")"

echo " "
echo "## Remove pgedge ################################################"

cd ..

if [ -d "pg16" ]; then
  ./nc remove pg16
fi

if [ -d "pg15" ]; then
  ./nc remove pg15
fi

if [ -d "postgrest" ]; then
  ./nc remove postgrest
fi

if [ -d "backrest" ]; then
  ./nc remove backrest
fi

if [ -d "bouncer" ]; then
  ./nc remove bouncer
fi 

